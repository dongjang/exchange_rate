{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://43.201.130.137:8081/api/$1"
    },
    {
      "source": "/oauth2/(.*)",
      "destination": "http://43.201.130.137:8081/oauth2/$1"
    },
    {
      "source": "/login/(.*)",
      "destination": "http://43.201.130.137:8081/login/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
